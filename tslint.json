{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "no-var-requires": false,
    "arrow-parens": false,
    "max-classes-per-file": [true, 10],
    "interface-name": false,
    "object-literal-sort-keys": false,
    "no-unused-variable": true
  },
  "rulesDirectory": []
}
